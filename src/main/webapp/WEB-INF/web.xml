<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register_HC</servlet-name>
        <servlet-class>org.health.health.Register_HC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register_VC</servlet-name>
        <servlet-class>org.health.vaccine.Register_VC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Booking</servlet-name>
        <servlet-class>org.health.booking.Booking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register_Admin</servlet-name>
        <servlet-class>org.health.administration.Register_Admin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register_HC</servlet-name>
        <url-pattern>/register_hc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register_VC</servlet-name>
        <url-pattern>/register_vc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register_Admin</servlet-name>
        <url-pattern>/register_admin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
